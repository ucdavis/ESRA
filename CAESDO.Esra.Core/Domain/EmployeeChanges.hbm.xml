<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="CAESDO.Esra.Core.Domain.EmployeeChanges, CAESDO.Esra.Core" table="EmployeeChanges" dynamic-update="true">
		<id name="id" access="field" column="EmployeeChangesID" unsaved-value="0">
			<generator class="identity" />
		</id>

    <property name="EmployeeID" column="FkEmployee" insert="false" update="false"/>
    <many-to-one name="Employee" class="CAESDO.Esra.Core.Domain.Employee, CAESDO.Esra.Core" column="FkEmployee" foreign-key="PkEmployee"/>
    <many-to-one name="Title" class="CAESDO.Esra.Core.Domain.Title, CAESDO.Esra.Core" column="TitleCode" foreign-key="TitleCode"/>
    <many-to-one name="User" class="CAESDO.Esra.Core.Domain.User, CAESDO.Esra.Core" column="UserID" foreign-key="UserID"/>
    <many-to-one name="ChangeType" class="CAESDO.Esra.Core.Domain.ChangeType, CAESDO.Esra.Core" column="ChangeTypeID" foreign-key="ChangeTypeID"/>
    <property name="DateChanged" column="DateChanged" />
		<property name="Comments" column="Comments" />
	</class>
</hibernate-mapping>