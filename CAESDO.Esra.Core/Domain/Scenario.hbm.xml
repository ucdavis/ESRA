<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="CAESDO.Esra.Core.Domain.Scenario, CAESDO.Esra.Core" table="Scenario" dynamic-update="true">
		<id name="id" access="field" column="ScenarioID" unsaved-value="0">
			<generator class="identity" />
		</id>
		
		<property name="ScenarioNumber" column="ScenarioNumber"/>
		<property name="PercentIncrease" column="PercentIncrease" />
		<property name="SalaryAmount" column="SalaryAmount" />
		<property name ="Approved" column="Approved"/>

		<many-to-one name="SalaryReviewAnalysis" class="CAESDO.Esra.Core.Domain.SalaryReviewAnalysis, CAESDO.Esra.Core" column="SalaryReviewAnalysisID"/>

		<many-to-one name="SelectionType" class="CAESDO.Esra.Core.Domain.SelectionType, CAESDO.Esra.Core" column="SelectionTypeID"/>
	</class>
</hibernate-mapping>