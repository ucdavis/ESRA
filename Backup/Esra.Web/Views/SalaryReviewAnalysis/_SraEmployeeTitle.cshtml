@model Esra.Core.Domain.SRAEmployee
<div>
    <table>
        <tr>
            <th>
                Title Code
            </th>
            <th>
                Payroll Title
            </th>
            <th>
                Salary Grade
            </th>
            <th>
                Salary Step
            </th>
            <th>
                Bargaining Unit
            </th>
            <th>
                Report Date
            </th>
        </tr>
        <tr>
            <td>@Model.Title.TitleCode
            </td>
            <td>@Model.Title.PayrollTitle
            </td>
            <td>@Model.SalaryGrade
            </td>
            <td>@Model.SalaryStep
            </td>
            <td>@Model.BargainingUnitCode
            </td>
            <td>@String.Format("{0:MM/dd/yyyy}", DateTime.Today)
            </td>
        </tr>
    </table>
</div>