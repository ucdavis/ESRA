<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Esra.Core.Domain.SalaryGrade, Esra.Core" table="SalaryGrades" dynamic-update="true">
    <!--<id name="Id" column="SalaryScaleID" unsaved-value="0">
			<generator class="identity" />
		</id>-->

    <composite-id unsaved-value="any">
      <key-property name="TitleCode" column="TitleCode"/>
      <key-property name="SalaryAdminPlan" column="SalaryAdminPlan"/>
      <key-property name="Grade" column="SalaryGrade"/>
      <key-property name="EffectiveDate" column="EffectiveDate"/>
    </composite-id>

    <property name="TitleCode" column="TitleCode" insert="false" update="false"/>
    <property name="SalaryAdminPlan" column="SalaryAdminPlan" insert="false" update="false"/>
    <property name="Grade" column="SalaryGrade" insert="false" update="false"/>
    <property name="EffectiveDate" column="EffectiveDate" insert="false" update="false"/>
    <property name="MinAnnual" column="MinAnnual" />
    <property name="FirstQrtleAnnual" column="FirstQrtleAnnual" />
    <property name="MidAnnual" column="MidAnnual" />
    <property name="ThirdQrtleAnnual" column="ThirdQrtleAnnual" />
    <property name="MaxAnnual" column="MaxAnnual" />

    <many-to-one name="SalaryScale" class="Esra.Core.Domain.SalaryScale, Esra.Core"
                 insert="false" update="false" cascade="none" unique="true">
      <column name="TitleCode"/>
      <column name="EffectiveDate"/>
    </many-to-one>

    <many-to-one name="Title" class="Esra.Core.Domain.Title, Esra.Core"
                 insert="false" update="false" cascade="none" unique="true">
      <column name="TitleCode"/>
    </many-to-one>

  </class>
</hibernate-mapping>